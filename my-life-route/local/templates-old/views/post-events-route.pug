.m-post-eventsroute
		.container
			.row
				.col-md-9.col-md-offset-3
					.heading-block
						.row
							.col-md-6.col-lg-6
								.breadcrumb-list
									ul
										li
											a(href="#") My Posts
										li
											a(href="#").active Events

							.col-md-6.col-lg-6
								.heading-block__buttons
									a(data-ui-sref="create-event").button.button-action.button-rounded.button-height-large.button-width-medium.margin-right-2x.margin-bottom-1x Create New 
									a(data-ui-sref="post-events-upcoming").button.button-green.button-rounded.button-height-large.button-width-medium.margin-bottom-1x  Upcoming
									a(data-ui-sref="post-events-expired").button.button-danger.button-rounded.button-height-large.button-width-medium.margin-left-2x.margin-bottom-1x  Expired
				.col-md-3
					.filter-side-block
						.filter-side-box(ng-class="{'active margin-bottom':issortingCollapsed}")
							.filter-block__top(ng-click="issortingCollapsed = !issortingCollapsed" class="pointer")
								h4 Sorting
								span.caret
							.filter-field-group(uib-collapse="!issortingCollapsed")
								.filter-inline-field
									.row.row-5
										.col-sm-3
											label Sort by
										.col-sm-9
											.f-field
												.select-control
														select.f-field-control
															option(value='Latest Post') Latest Post
									.filter-button-group
										.row.row-10
											.col-md-6.col-sm-6
												button.button.button-rounded.button-block.margin-bottom-2x Apply
											.col-md-6.col-sm-6
												button.button.button-border.button-theme.button-border-thin.button-rounded.button-block.margin-bottom-2x Reset
										
										
									

						.filter-side-box(ng-class="{'active margin-top':isfilterCollapsed}")
							.filter-block__top(ng-click="isfilterCollapsed = !isfilterCollapsed" class="pointer")
								h4 Filters
								span.caret
							.filter-field-group(uib-collapse="!isfilterCollapsed")
								.f-field
									input(type="text", placeholder="Title").f-field-control
								.f-field.has-icon
									input(type="text", placeholder="From").f-field-control
									span.icon-addon
										i.icon-calendar
								.f-field.has-icon
									input(type="text", placeholder="To").f-field-control
									span.icon-addon
										i.icon-calendar
								.f-field
									input(type="text", placeholder="Location").f-field-control
								
								a(href="#" class="search_link") Advanced Search
								.filter-button-group
										.row.row-10
											.col-md-6.col-sm-6
												button.button.button-rounded.button-block.margin-bottom-2x Apply
											.col-md-6.col-sm-6
												button.button.button-border.button-theme.button-border-thin.button-rounded.button-block.margin-bottom-2x Reset

			

				.col-md-9
					
									
					//.ap-filter remove class if filter not applied				
					.m-post-eventroute__list.ap-filter
						.filter-ap-msg
							h4  
								span
									i.icon-asterisk-symbol 
								|Filters Applied
						.total-posted
							span Total Posted : 20
